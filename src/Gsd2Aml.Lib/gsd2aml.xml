    <Reference Ref="$3" Type="InlineRef">
      <InterfaceSubmoduleItem IsochroneModeInRT_Classes="" />
    </Reference>
    <Reference Ref="$4" Type="InlineRef">
      <InterfaceSubmoduleItem SupportedProtocols="" />
    </Reference>
    <Reference Ref="$5" Type="InlineRef">
      <InterfaceSubmoduleItem SupportedMibs="" />
    </Reference>
    <Reference Ref="$6" Type="InlineRef">
      <InterfaceSubmoduleItem SubslotNumber="" />
    </Reference>
    <Reference Ref="$7" Type="InlineRef">
      <InterfaceSubmoduleItem SubmoduleIdentNumber="" />
    </Reference>
    <Reference Ref="$8" Type="InlineRef">
      <InterfaceSubmoduleItem DCP_HelloSupported="" />
