          <WriterProjectTitle>GSD2AML Converter</WriterProjectTitle>
          <WriterProjectID>GSD2AML Converter</WriterProjectID>
        </WriterHeader>
      </AdditionalInformation>
    </Replacement>
  </ProfileHeader>

  <ProfileBody>
    <Replacement>
      <CAEXFile.SystemUnitClassLib>
        <SystemUnitClassLib CAEXObject.Name="ComponentSystemUnitClassLib" />
      </CAEXFile.SystemUnitClassLib>
    </Replacement>
  </ProfileBody>

  <DeviceIdentity>
    <Replacement>
      <CAEXFile.SystemUnitClass>
        <SystemUnitClass CAEXObject.Name="$1" CAEXObject.ID="GUID">

          <SystemUnitClassType.SupportedRoleClass>
            <SupportedRoleClass SupportedRoleClass.RefRoleClassPath="AutomationMLComponentBaseRCL/AdditionalDeviceDescription/PNGSD" />
          </SystemUnitClassType.SupportedRoleClass>

          <SystemUnitClassType.InternalElement>
            <InternalElement CAEXObject.Name="DeviceIdentification" CAEXObject.ID="GUID">
              <SystemUnitClassType.Attribute>
                <Attribute CAEXObject.Name="Manufacturer" Attribute.AttributeDataType="xs:string">
                  <Attribute.Value>$2</Attribute.Value>
                </Attribute>

                <Attribute CAEXObject.Name="Model" Attribute.AttributeDataType="xs:string">
                  <Attribute.Value>$1</Attribute.Value>
                </Attribute>

                <Attribute CAEXObject.Name="ProductCode" Attribute.AttributeDataType="xs:string">
                  <Attribute.Value>$1</Attribute.Value>
                </Attribute>

                <Attribute CAEXObject.Name="DeviceClass" Attribute.AttributeDataType="xs:string">
                  <Attribute.Value>I/O</Attribute.Value>
                </Attribute>

                <Attribute CAEXObject.Name="ShortDescription" Attribute.AttributeDataType="xs:string">
                  <Attribute.Value>$4</Attribute.Value>
                </Attribute>

                <Attribute CAEXObject.Name="ArticleIdentificationOfVendor" Attribute.AttributeDataType="xs:string">
                  <Attribute.Value>$1</Attribute.Value>
                </Attribute>
              </SystemUnitClassType.Attribute>
            </InternalElement>
            <InternalElement CAEXObject.Name="GSDML" CAEXObject.ID="GUID">

              <SystemUnitClassType.SupportedRoleClass>
                <SupportedRoleClass SupportedRoleClass.RefRoleClassPath="AutomationMLComponentBaseRCL/AdditionalDeviceDescription" />
              </SystemUnitClassType.SupportedRoleClass>

              <SystemUnitClassType.ExternalInterface>
                <ExternalInterface CAEXObject.Name="GSDML 2.35" CAEXObject.ID="GUID" ExternalInterface.RefBaseClassPath="AutomationMLInterfaceClassLib/AutomationMLBaseInterface/ExternalDataConnector">
                  <ExternalInterface.Attribute>
                    <Attribute CAEXObject.Name="refURI" Attribute.AttributeDataType="xs:anyURI">
                      <Value>$5</Value>
                    </Attribute>
                    <Attribute CAEXObject.Name="MIMEType" Attribute.AttributeDataType="xs:string">
                      <Value>application/xml</Value>
                    </Attribute>
                  </ExternalInterface.Attribute>
                </ExternalInterface>
              </SystemUnitClassType.ExternalInterface>

            </InternalElement>

            <HEAD>
              <InternalElement Name="PhysicalInterfaces" ID="GUID" />
            </HEAD>
          </SystemUnitClassType.InternalElement>

        </SystemUnitClass>
      </CAEXFile.SystemUnitClass>
    </Replacement>
    <!-- Caution! There can be multiple DeviceAccessPointItems in a DeviceAccessPointList. Just take the first one, it shouldn,t matter which one you choose regardless. -->
    <Reference Ref="$1" Type="SingleTextRef" Internal="false">
      <ISO15745Profile>
        <ProfileBody>
          <ApplicationProcess>
            <DeviceAccessPointList>
              <DeviceAccessPointItem>
                <ModuleInfo>
                  <Name TextId="" />
                </ModuleInfo>
              </DeviceAccessPointItem>
            </DeviceAccessPointList>
          </ApplicationProcess>
        </ProfileBody>
      </ISO15745Profile>
    </Reference>
    <Reference Ref="$2" Type="DirectRef" Internal="true">
      <DeviceIdentity>
        <VendorName Value="" />
      </DeviceIdentity>
    </Reference>
    <Reference Ref="$3" Type="DirectRef" Internal="false">
      <ISO15745Profile>
        <ProfileBody>
          <DeviceFunction>
            <Family MainFamily="" />
          </DeviceFunction>
        </ProfileBody>
      </ISO15745Profile>
    </Reference>
    <!-- Again, just choose the first one. -->
    <Reference Ref="$4" Type="SingleTextRef" Internal="false">
      <ISO15745Profile>
        <ProfileBody>
          <ApplicationProcess>
            <DeviceAccessPointList>
              <DeviceAccessPointItem>
                <ModuleInfo>
                  <InfoText TextId="" />
                </ModuleInfo>
              </DeviceAccessPointItem>
            </DeviceAccessPointList>
          </ApplicationProcess>
        </ProfileBody>
      </ISO15745Profile>
    </Reference>
    <Reference Ref="5" Type="GSDRef" />
  </DeviceIdentity>

